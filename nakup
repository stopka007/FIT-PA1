#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>

int main (void) {
    char string[10000];
    char seznam[100][10000];
    char podminka1[1][10] = {"#0\n"};
    char slovo[10000];
    int n = 0;
    int i = 0;
    char hashtag = '#';
    int regal = '0';
    int nakupni_list = 0;
    while (fgets(string, sizeof string, stdin) != NULL) {
        strcpy(seznam[n], string);
        strcpy(slovo, string);
        if (slovo[i] == '\n') {
            nakupni_list += 1;
        } 
        if (strcmp(seznam[0],*podminka1) != 0) { // pokud prvni radek neni #0
            printf("Nespravny vstup.\n");
            return 0;
        }
        while (slovo[i] != '\n'){ // zkouska aby regaly byly vzestupne
            if (slovo[i] == hashtag && slovo[i+1] == regal){ 
                regal += 1;
            }
            else if (slovo[i] == hashtag && slovo[i+1] != regal){ 
                printf("Nespravny vstup.\n");
                return 0;
            }
            i += 1;
        }
        n+=1;
        i=0;
    }
    if (nakupni_list == 0) {
        printf("Nespravny vstup.\n");
        return 0;
    }
    //for (int i = 0; i < n; i++){
    //    printf("na pozici %d je %s\n", i, seznam[i]);
    //}
    return 0;
}
