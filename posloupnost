#include <stdio.h>
#include <stdlib.h>

int main(){
    int cislo, input, suma = 0, dvojice = 0, n = 0, index = 0, sort;

    int vsechna_cisla[2000];
    int pole_sum[2000000];

    printf("Posloupnost:\n");

    while( n < 2000){                                           
        if ((input = scanf("%d", &cislo)) == EOF ){                  
            break;
        }
        else if (input != 1){
            printf("Nespravny vstup.\n");
            break;
        }
        else {
            vsechna_cisla[n] = cislo;
            n += 1;
        }
    }
    for (int i = 0; i < n-1; i++){ //vytvoreni pole sum
        suma = vsechna_cisla[i];
        for (int l = 1+i; l < n; l++){
            suma += vsechna_cisla[l];
            pole_sum[index] = suma;
            index += 1;                                          
        }
        suma = 0;
    }
    for (int i = 0; i < index; i++){ // sort pole sum
        for(int n = i+1; n < index; n++){
            if(pole_sum[i] > pole_sum[n]){
                sort = pole_sum[i];
                pole_sum[i] = pole_sum[n];
                pole_sum[n] = sort;
            }
        }
    }
    for (int i = 0; i < index-1 ; i++){ // tvoreni dvojic z pole sum
        for (int l = 1+i; l < index; l++){
           if (pole_sum[i] == pole_sum[l]){
            dvojice += 1;
           }
           else{
            continue;
           }                 
        }
    }
    if (input == -1){
        printf("Pocet dvojic: %d\n", dvojice);
    }
    return 0;
}
