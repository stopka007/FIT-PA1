#include <stdio.h>
#include <stdlib.h>

int main(){
    int cislo, input, suma = 0, dvojice = 0, n = 0, index = 0, sort;

    int vsechna_cisla[2000];
    int pole_sum[2000000];

    printf("Posloupnost:\n");

    while( n < 2000){                                           
        if ((input = scanf("%d", &cislo)) == EOF || n >= 2000 ){                  
            break;
        }
        else if (input != 1){
            printf("Nespravny vstup.\n");
            break;
        }
        else {
            vsechna_cisla[n] = cislo;
            n += 1;
        }
    }
    if (n == 0 && input != 0){
        printf("Nespravny vstup.\n");
    }
    for (int i = 0; i < n-1; i++){
        suma = vsechna_cisla[i];
        for (int l = 1+i; l < n; l++){ //vytvoreni pole sum
            suma += vsechna_cisla[l];
            pole_sum[index] = suma;
            for (int k = index; k > 0; k--){ // sort
                if(pole_sum[k] < pole_sum[k-1]){
                    sort = pole_sum[k];
                    pole_sum[k] = pole_sum[k-1];
                    pole_sum[k-1] = sort;
                }
            }
            index += 1;
        }
        suma = 0;
    }
    for (int i = 0; i < index-1 ; i++){ // tvoreni dvojic
        for (int l = 1+i; l < index; l++){
           if (pole_sum[i] == pole_sum[l]){
            dvojice += 1;
           }
           else{
            break;
           }                 
        }
    }
    if (n > 0){
        printf("Pocet dvojic: %d\n", dvojice);
    }
    return 0;
}
